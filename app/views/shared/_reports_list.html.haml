%table.dashboard.table.table-striped.table-bordered
  %tbody
  - current_user.all_reports.each do |report|
    %tr
      %td
        = link_to(report.name, report.url)
      %td
        [
        = link_to("Edit", report_path(report))
        ]

.links
  = link_to("Manage all Reports", reports_path)


