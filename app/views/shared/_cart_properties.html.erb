<table width="100%" class="data_container cart_properties">
  <tr class='header'>
    <td class="first" width="33%" scope="col" colspan="2">
      <h5>FY15 Credit Card Purchase Request</h5>
    </td>
  </tr>
  <%- cart.deserialized_properties.each do |property,value| %>
    <%- unless cart.property_exclusions.include? property %>
      <tr class="cart_item_information">
        <td class="w-container results-list" align="left" width="33%">
          <strong><%= property.underscore.humanize %></strong>
        </td>
        <td with="66%">
          <%= value %>
        </td>
      </tr>
    <%- end %>
  <%- end %>
</table>
