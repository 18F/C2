<% content_for :title, "My Requests" %>
<% content_for :top_notification do %>
  <% render partial: "proposals/details/top_notification" %>
<% end %>
<meta name="list_view_config" content='<%= @list_view_config.html_safe %>'>

<div id="proposals-unfiltered" class="proposals-unfiltered">
  <div class="medium-12 large-6 large-push-6 column">
    <% if display_search_ui? %>
      <%= render partial: "shared/search_ui", locals: { show_adv_search_ui: params[:search] } %>
    <% end %>
  </div>
  <div class="medium-12 large-6 large-pull-6 column">
    <h3>Requests</h3>
  </div>
  <div class="medium-12 column" style='clear:both'>
    <div class="sk-fading-circle">
      <div class="sk-circle1 sk-circle"></div>
      <div class="sk-circle2 sk-circle"></div>
      <div class="sk-circle3 sk-circle"></div>
      <div class="sk-circle4 sk-circle"></div>
      <div class="sk-circle5 sk-circle"></div>
      <div class="sk-circle6 sk-circle"></div>
      <div class="sk-circle7 sk-circle"></div>
      <div class="sk-circle8 sk-circle"></div>
      <div class="sk-circle9 sk-circle"></div>
      <div class="sk-circle10 sk-circle"></div>
      <div class="sk-circle11 sk-circle"></div>
      <div class="sk-circle12 sk-circle"></div>
    </div>
    <% if @unfiltered_data.rows.any? %>
      <%= render partial: "shared/table_next", locals: { container: @unfiltered_data } %>
    <% else %>
      <p class="empty-list-label">No purchase requests</p>
    <% end %>
  </div>
</div>
