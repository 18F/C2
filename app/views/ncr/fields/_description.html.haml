.column{ id: key + '-wrapper'}
  .detail-wrapper.row{ class: key + "-wrapper", id: value_id }
    .detail-display.column
      %label.detail-element
        = t(t_slug + key)
      %span.detail-value
        = client_field
    .detail-edit.column
      %label.detail-element{ for: "ncr_work_order_description" }
        = t(t_slug + key)
      %span.detail-value
        = f.input :description, label: false
