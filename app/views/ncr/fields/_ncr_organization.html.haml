.column{ id: key + '-wrapper'}
  .detail-wrapper.row{ class: key + "-wrapper", id: value_id }
    .detail-display.column
      %label.detail-element
        = t(t_slug + key) 
      %span.detail-value
        = client_data[key]
    .detail-edit.column
      %label.detail-element
        = t(t_slug + key) 
      %span.detail-value
        = f.association :ncr_organization,
          as: :select,
          prompt: :translate,
          input_html: { class: "js-selectize", data: { initial: JSON.generate(organization_options) }},
          label: false
